page {
  background-color: #f5f5f5;
}

.edit {
  padding: 15rpx 25rpx;
  font-size: 28rpx;
  color: #999;
  background-color: #fff;
}

.cart-shop {
  padding: 20rpx 25rpx;
  background-color: #fff;
}

.cart-shop image {
  width: 28rpx;
  height: 26rpx;
}

.cart-shop text {
  font-size: 32rpx;
  color: #010101;
  padding-left: 10rpx;
  padding-right: 10rpx;
}

.cart-shop icon {
  margin-right: 15rpx;
}

.iconch {
  border: 1px solid #a9a9a9;
  border-radius: 50px;
}

.iconchbox .iconch {
  display: none;
}

.iconchbox .iconch2 {
  display: block;
}

.iconchbox2 .iconch2 {
  display: none;
}

.iconchbox2 .iconch {
  display: block;
}

.shoplist-image {
  width: 150rpx;
  height: 150rpx;
  border: 1px solid #f2f2f2;
  margin-left: 20rpx;
}

.shoplist-image image {
  width: 100%;
  height: 100%;
}

.shoplist-cont {
  flex: 1;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  margin-left: 15rpx;
}

.shoplist {
  padding: 10rpx 25rpx 15rpx 25rpx;
  background-color: #fff;
}

.shoplist-conttitle {
  font-size: 28rpx;
  color: #333;
  padding-bottom: 5rpx;
}

.shoplist-contlabel {
  color: #999;
  font-size: 24rpx;
}

.shoplist-contprice {
  font-size: 26rpx;
}

.shoplist-contprice text {
  font-size: 24rpx;
  color: #999;
  position: relative;
  margin-left: 27rpx;
}

.shoplist-contprice text:before {
  content: "";
  position: absolute;
  top: 50%;
  background-color: #999;
  width: 100%;
  height: 1px;
}

.shopboxlist {
  background-color: #fff;
  margin-bottom: 15rpx;
  padding-bottom: 10rpx;
}

.cartnav {
  position: fixed;
  bottom: 0;
  width: 100%;
  display: flex;
  background-color: #fff;
  border-top: 1px solid #f2f2f2;
  height:100rpx;
  align-items: center;
  justify-content: space-between;

}
.cartnav-ch{
 margin-left: 30rpx;
}
.cartnav-jies{
  background-color: #C02D3A;
  color: #fff;
  height: 100rpx;
  width: 170rpx;
  text-align: center;
  line-height: 100rpx;
  font-size: 30rpx;
}
.dibu{
  width: 100%;
  height: 100rpx;
}
.cartnav-hej{
  font-size: 30rpx;
}
.cartnav-hej text{
  color: #C02D3A;
}